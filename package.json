{
  "name": "codehills-be",
  "version": "1.0.0",
  "description": "backend for codehills project",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/app.js",
    "build": "babel src --out-dir dist --source-maps inline --copy-files",
    "start": "babel-node  dist/app.js",
    "test": "mocha --require @babel/register 'src/tests/**/*.test.js'",
    "test:coverage": "nyc mocha --coverage --recursive --require @babel/polyfill --require @babel/register */tests/*.js --timeout 500000000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denislohan/codehills-be.git"
  },
  "author": "codehills-devs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/denislohan/codehills-be/issues"
  },
  "homepage": "https://github.com/denislohan/codehills-be#readme",
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "body-parser": "^1.20.1",
    "coveralls": "^3.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nyc": "^15.1.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^5.22.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^5.5.1"
  }
}
